<UserControl x:Class="FishComponents.Particle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <UserControl.Resources>
            <BitmapImage x:Key="AvailablePng" UriSource="images/available.png"/>
            <BitmapImage x:Key="BusyPng" UriSource="images/Busy.png"/>
            <BitmapImage x:Key="AwayPng" UriSource="images/Away.png"/>
            <BitmapImage x:Key="BlockedPng" UriSource="images/blocked.png"/>
            <BitmapImage x:Key="DndPng" UriSource="images/Dnd.png"/>
            <BitmapImage x:Key="InactivePng" UriSource="images/Inactive.png"/>
            <BitmapImage x:Key="InMtgPng" UriSource="images/InMtg.png"/>
            <BitmapImage x:Key="OfflinePng" UriSource="images/Offline.png"/>
            <BitmapImage x:Key="UnknownPng" UriSource="images/Unknown.png"/>
        
    </UserControl.Resources>
    <Grid>
        <Image x:Name="imgStatus" />
    </Grid>
</UserControl>
